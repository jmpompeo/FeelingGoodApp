@model FeelingGoodApp.Models.IndexViewModel

@{
    ViewData["Title"] = "Index";
}

<style>
    .button {
        border-top: 1px solid #5dd465;
        background: #13344a;
        background: -webkit-gradient(linear, left top, left bottom, from(#76abcf), to(#13344a));
        background: -webkit-linear-gradient(top, #76abcf, #13344a);
        background: -moz-linear-gradient(top, #76abcf, #13344a);
        /*    background: -ms-linear-gradient(top, #76abcf, #13344a);*/
        background: -o-linear-gradient(top, #76abcf, #13344a);
        padding: 8.5px 17px;
        -webkit-border-radius: 22px;
        -moz-border-radius: 22px;
        border-radius: 22px;
        -webkit-box-shadow: rgba(0,0,0,1) 0 1px 0;
        -moz-box-shadow: rgba(0,0,0,1) 0 1px 0;
        box-shadow: rgba(0,0,0,1) 0 1px 0;
        text-shadow: rgba(0,0,0,.4) 0 1px 0;
        color: #ededed;
        font-size: 18px;
        font-family: Georgia, serif;
        text-decoration: none;
        vertical-align: middle;
    }
        .button:hover {
            border-top-color: #90ab91;
            background: #90ab91;
            color: #091821;
        }
        .button:active {
            border-top-color: #1b435e;
            background: #1b435e;
        }
</style>

    <a class="button"  asp-area="Identity" asp-page="/Account/Register">Register To Feel Good</a><br /><br />
    <a class="button" asp-area="Identity" asp-page="/Account/Login">Login To Feel Great</a>
